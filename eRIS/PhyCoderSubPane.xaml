                    <TextBox x:Name="iModifier1" HorizontalAlignment="Left" Width="60" Margin="1,1,1,1"/>
                    <TextBox x:Name="iICD1" HorizontalAlignment="Left" Width="166" Margin="1,1,1,1" />
